{
  "name": "wyrihaximus/react-awaitable-observable",
  "description": "\ud83d\udee0\ufe0f Make observables foreachable using async &amp; await",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "react/async": "dev-main",
    "react/promise": "^2.8",
    "reactivex/rxphp": "^2.0"
  },
  "require-dev": {
    "wyrihaximus/async-test-utilities": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "WyriHaximus\\React\\": "src/"
    },
    "files": [
      "src/functions_include.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\React\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "ergebnis/composer-normalize": true,
      "icanhazstring/composer-unused": true,
      "infection/extension-installer": true
    },
    "platform": {
      "php": "8.1.1"
    },
    "sort-packages": true
  },
  "extra": {
    "unused": [
      "php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
